<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.matheusfroes.miniprojeto03">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.ListaFuncionariosActivity"
            android:label="@string/funcionarios"
            android:parentActivityName=".activities.MainActivity" />


        <activity
            android:label="@string/detalhes_funcionario"
            android:name=".activities.DetalhesFuncionario"
            android:parentActivityName=".activities.ListaFuncionariosActivity" />
    </application>

</manifest>